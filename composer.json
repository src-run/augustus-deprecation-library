{
  "name"        : "src-run/augustus-deprecation-library",
  "license"     : "MIT",
  "type"        : "library",
  "homepage"    : "https://src.run/augustus-deprecation-library",
  "description" : "This library contains basic deprecation handler functionality.",
  "support" : {
    "email"  : "rmf@src.run",
    "issues" : "https://src.run/augustus-deprecation-library/issues",
    "source" : "https://src.run/augustus-deprecation-library/git"
  },
  "keywords" : [
      "augustus-deprecation-library", "augustus", "deprecation", "library", "rmf", "src-run"
  ],
  "authors" : [
    {
      "name"    : "Rob Frawley 2nd",
      "email"   : "rmf@src.run",
      "homepage": "https://src.run/rmf",
      "role"    : "Project Lead"
    }
  ],
  "require" : {
    "php"                                : "^5.6|^7.0",
    "src-run/augustus-exception-library" : "^0.6"
  },
  "require-dev" : {
    "phpunit/phpunit"        : "^5.0",
    "codacy/coverage"        : "^1.0",
    "satooshi/php-coveralls" : "^1.0",
    "symfony/var-dumper"     : "^3.0"
  },
  "autoload" : {
    "psr-4" : {
      "SR\\Deprecation\\" : "lib/"
    }
  },
  "autoload-dev" : {
    "psr-4" : {
      "SR\\Tests\\Deprecation\\" : "tests/"
    }
  },
  "minimum-stability" : "dev",
  "prefer-stable"     : true,
  "config": {
    "bin-dir" : "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master" : "0.6-dev"
    }
  }
}
